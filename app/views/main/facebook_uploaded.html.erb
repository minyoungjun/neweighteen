<section class="section-wrap blog-standard blog-article">
  <div class="container relative">
    <div class="row">
      
      <!-- content -->
      <div class="col-md-10 col-md-offset-1">

        <!-- gallery post -->
        <div class="entry-item mb0">
          <div class="entry">             
            <div class="entry-title" style="text-align:center;">
              <h2>
                다음 Facebook 영상이 등록되었습니다.
              </h2>
            </div>
            <div class="entry-content">
              <div class="fb-post" data-href="<%=@facebook.url%>" data-width="500"></div>
              <br>

              <a href="/upload/facebook"> 
                <div class="btn btn-large btn-pink" >더 올리기</div>
              </a>
            </div> <!-- end entry content -->
          </div> <!-- end entry -->
        </div> <!-- end entry item -->
        
      </div> <!-- end col -->

    </div> <!-- end row -->
  </div> <!-- end container -->
</section> <!-- end blog single -->
